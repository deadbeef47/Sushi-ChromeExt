{
    "manifest_version": 2,
    "name": "",
    "description": "",
    "version": "1.0",
    "icons": {
        "32": "img/sushi_aburi_toro.png"
    },
    "content_scripts": [{
        "matches": ["http://*/*", "https://*/*" ],
        "css": ["content_scripts.css"],
        "js": ["jquery.min.js", "content_scripts.js"]
    }],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action" : {
        "default_icon": "img/sushi_aburi_toro.png",
        "default_title": "",
         "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "background",
        "http://*/*",
        "https://*/*"
    ],
    "web_accessible_resources": [
        "img/*"
    ]
}
